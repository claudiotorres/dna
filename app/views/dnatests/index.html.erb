<h1>Listing dnatests</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Date</th>
    <th>Site</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dnatests.each do |dnatest| %>
  <tr>
    <td><%= dnatest.name %></td>
    <td><%= dnatest.description %></td>
    <td><%= dnatest.date %></td>
    <td><%= dnatest.site %></td>
    <td><%= link_to 'Show', dnatest %></td>
    <td><%= link_to 'Edit', edit_dnatest_path(dnatest) %></td>
    <td><%= link_to 'Destroy', dnatest, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

